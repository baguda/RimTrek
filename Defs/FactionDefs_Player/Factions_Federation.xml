<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <FactionDef ParentName="PlayerFactionBase">
    <defName>FederationPlayer</defName>
    <label>Federation Frontier Outpost</label>
    <description>A federation outpost for exploration and diplomacy.</description>
    <isPlayer>true</isPlayer>
    <basicMemberKind>PlayerFedHum</basicMemberKind>
    <pawnSingular>citizen</pawnSingular>
    <pawnsPlural>citizens</pawnsPlural>
    <techLevel>Spacer</techLevel>
    <backstoryCategories>
      <li>Federation</li>
    </backstoryCategories>
    <factionIconPath>World/UFP</factionIconPath>
    <playerInitialSettlementNameMaker>NamerInitialSettlementColony</playerInitialSettlementNameMaker>
    <factionNameMaker>NamerFactionFederation</factionNameMaker>
    <settlementNameMaker>NamerSettlementOutlander</settlementNameMaker>
    <geneticVariance>1</geneticVariance>
    <startingResearchTags>
      <li>ClassicStart</li>
      <li>ShipRelated</li> 
    </startingResearchTags>
    <hairTags>
      <li>Rural</li>
    </hairTags>
    <apparelStuffFilter>
      <thingDefs>
        <li>Synthread</li>
      </thingDefs>
    </apparelStuffFilter>
  </FactionDef>
  
<FactionDef>
		<defName>FederationAlly</defName>
		<label>United Federation of Planets</label>
    <description>A colony of the United Federation of Planets.</description>
    <pawnsPlural>Federation Citizens</pawnsPlural>
	<fixedName>The United Federation of Planets</fixedName>
    <settlementGenerationWeight>0</settlementGenerationWeight>
    <requiredCountAtGameStart>1</requiredCountAtGameStart>
    <canMakeRandomly>false</canMakeRandomly>
    <canSiege>false</canSiege>
    <canStageAttacks>false</canStageAttacks>
    <leaderTitle>Chief Expert</leaderTitle>
    <colorSpectrum>
      <li>(1, 0.89, 0.44)</li>
    </colorSpectrum>
    <startingGoodwill>
      <min>100</min>
      <max>100</max>
    </startingGoodwill>
    <pawnNameMaker>NamerPersonTribal</pawnNameMaker>
	<settlementNameMaker>NamerFactionFederation</settlementNameMaker>
	<techLevel>Industrial</techLevel>
	<backstoryCategories><li>Federation</li></backstoryCategories>
	<hairTags>
			<li>Urban</li>
			<li>Punk</li>
			<li>Rural</li>
			<li>Tribal</li>
	</hairTags>
    <naturalColonyGoodwill>
      <min>100</min>
      <max>100</max>
    </naturalColonyGoodwill>
    <visitorTraderKinds>
      <li>Visitor_Federation_Standard</li>
    </visitorTraderKinds>
   <caravanTraderKinds>
      <li>Caravan_Outlander_BulkGoods</li>
      <li>Caravan_Outlander_CombatSupplier</li>
      <li>Caravan_Outlander_Exotic</li>
    </caravanTraderKinds>
    <baseTraderKinds>
      <li>Base_Outlander_Standard</li>
    </baseTraderKinds>
    <allowedArrivalTemperatureRange>
      <min>-30</min>
      <max>45</max>
    </allowedArrivalTemperatureRange>
	<maxPawnCostPerTotalPointsCurve>
      <points>
        <li>(0,35)</li>
        <li>(70, 50)</li>
        <li>(700, 100)</li>
        <li>(1300, 150)</li>
        <li>(100000, 10000)</li>
      </points>
    </maxPawnCostPerTotalPointsCurve>
    <pawnGroupMakers>
      <li>
        <kindDef>Peaceful</kindDef>
        <options>
          <AVulcanColonist>3</AVulcanColonist>
          <ATellariteColonist>3</ATellariteColonist>
          <AAndorianColonist>3</AAndorianColonist>
          <AFedHumColonist>3</AFedHumColonist>
          <AAndorianSecurityGuard>10</AAndorianSecurityGuard>
          <AVulcanElite>1</AVulcanElite>
          <AFedHumRecruit>5</AFedHumRecruit>
          <AAndorianRecruit>5</AAndorianRecruit>
		          <AVulcanRecruit>5</AVulcanRecruit>
          <ATellariteRecruit>5</ATellariteRecruit>
          <AVulcanCommandoElite>1</AVulcanCommandoElite>
        </options>
      </li>
	  <li>
      <kindDef>Trader</kindDef>
        <traders>
          <AFedHumTrader>1</AFedHumTrader>
        </traders>
        <carriers>
          <Muffalo>2</Muffalo>
          <Dromedary>2</Dromedary>
        </carriers>
        <guards>
          <AAndorianSecurityGuard>9</AAndorianSecurityGuard>
          <AAndorianHunter>1</AAndorianHunter>
          <AFedHumCommando>5</AFedHumCommando>
          <AFedHumCommandoElite>2</AFedHumCommandoElite>
        </guards>
      </li>
	  <li>
        <kindDef>Combat</kindDef>
        <options>
          <AAndorianSecurityGuard>10</AAndorianSecurityGuard>
          <AAndorianCommandoElite>1</AAndorianCommandoElite>
          <AFedHumCommando>5</AFedHumCommando>
          <AVulcanElite>1</AVulcanElite>
          <AFedHumRecruit>5</AFedHumRecruit>
          <AVulcanRecruit>5</AVulcanRecruit>
		          <AAndorianRecruit>5</AAndorianRecruit>
          <ATellariteRecruit>5</ATellariteRecruit>
          <AVulcanCommandoElite>1</AVulcanCommandoElite>
        </options>
      </li>
	  <li>
        <kindDef>Settlement</kindDef>
        <options>
          <ATellariteSecurity>10</ATellariteSecurity>
          <AVulcanHunter>1</AVulcanHunter>
          <AAndorianCommando>5</AAndorianCommando>
          <AVulcanColonist>3</AVulcanColonist>
          <ATellariteColonist>3</ATellariteColonist>
          <AAndorianColonist>3</AAndorianColonist>
          <AFedHumColonist>3</AFedHumColonist>
          <AVulcanElite>1</AVulcanElite>
          <AFedHumRecruit>5</AFedHumRecruit>
          <AAndorianRecruit>5</AAndorianRecruit>
		   <AVulcanRecruit>5</AVulcanRecruit>
          <ATellariteRecruit>5</ATellariteRecruit>
        </options>
      </li>
    </pawnGroupMakers>
    <settlementTexturePath>World/FedBaseIcon</settlementTexturePath>
    <factionIconPath>World/FedBaseIcon</factionIconPath>
	<apparelStuffFilter>
		<thingDefs>
			<li>Synthread</li>
			<li>Hyperweave</li>
		</thingDefs>
	</apparelStuffFilter>
  </FactionDef>


<?xml version="1.0" encoding="utf-8" ?>
<Defs>

   <!-- ============================ Andorian Names ========================== -->
  <RulePackDef>
    <defName>NamerAndorianUtility</defName>
    <rulePack>
      <rulesFiles>
        <li>AndFirstMale->NameBanks/Names_First_Andorian_Male</li>
        <li>AndFirstFemale->NameBanks/Names_First_Andorian_Female</li>
        <li>AndLast->NameBanks/Names_Last_Andorian</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>NamerAndorianMale</defName>
    <include>
      <li>NamerAndorianUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name->[AndFirstMale] [AndLast]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerAndorianFemale</defName>
    <include>
      <li>NamerAndorianUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name->[AndFirstFemale] [AndLast]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  

</Defs>